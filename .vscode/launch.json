    "version": "0.2.0",
    "configurations": [
        {
            "name": "target-dualentry",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": false,
            "program": "${workspaceFolder}/target_dualentry/target.py",
            "cwd": "${workspaceFolder}/.secrets",
            "args": [
                "<",
                "${workspaceFolder}/.secrets/data.singer",
                "--config",
                "config.json"
            ],
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:$PYTHONPATH",
                "FLOW": "A8GglKB3a",
                "SNAPSHOT_DIR": "${workspaceFolder}/.secrets/snapshots"
            }
        }
    ],
    
}